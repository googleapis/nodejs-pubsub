    "async": "^2.6.0",
    "codecov": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": "^2.3.1",
    "ink-docstrap": "^1.3.2",
    "intelli-espower-loader": "^1.0.1",
    "jsdoc": "^3.5.5",
    "mocha": "^5.0.0",
    "nyc": "^11.3.0",
    "power-assert": "^1.4.4",
    "prettier": "^1.9.1",
    "proxyquire": "^2.0.0"
  }
