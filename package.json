    "google-gax": "^0.16.0",
    "google-proto-files": "^0.15.0",
    "is": "^3.0.1",
    "lodash.chunk": "^4.2.0",
    "lodash.merge": "^4.6.0",
    "lodash.snakecase": "^4.1.1",
    "protobufjs": "^6.8.1",
    "through2": "^2.0.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
