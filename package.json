  "license": "Apache-2.0",
  "author": "Google Inc.",
  "engines": {
    "node": ">=18"
  },
  "repository": "googleapis/nodejs-pubsub",
  "main": "./build/src/index.js",
  "types": "./build/src/index.d.ts",
  "files": [
    "build/protos",
    "build/src"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google pubsub",
    "pubsub"
