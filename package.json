    "arrify": "^2.0.0",
    "extend": "^3.0.2",
    "google-auth-library": "^7.0.0",
    "google-gax": "^2.12.0",
    "is-stream-ended": "^0.1.4",
    "lodash.snakecase": "^4.1.1",
    "p-defer": "^3.0.0"
  },
  "devDependencies": {
    "@grpc/proto-loader": "^0.6.0",
    "@microsoft/api-documenter": "^7.8.10",
    "@microsoft/api-extractor": "^7.8.10",
    "@opentelemetry/tracing": "^0.18.0",
    "@types/execa": "^0.9.0",
    "@types/extend": "^3.0.0",
    "@types/lodash.snakecase": "^4.1.6",
    "@types/mocha": "^8.0.0",
    "@types/mv": "^2.1.0",
    "@types/ncp": "^2.0.1",
    "@types/node": "^12.12.30",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^10.0.0",
    "@types/tmp": "^0.2.0",
    "@types/uuid": "^8.0.0",
    "c8": "^7.0.0",
    "codecov": "^3.0.0",
    "execa": "^5.0.0",
    "gts": "^2.0.0",
    "jsdoc": "^3.6.2",
    "jsdoc-fresh": "^1.0.1",
    "jsdoc-region-tag": "^1.0.2",
    "linkinator": "^2.0.0",
    "mocha": "^8.0.0",
    "mv": "^2.1.1",
    "ncp": "^2.0.0",
    "null-loader": "^4.0.0",
    "protobufjs": "^6.10.1",
    "proxyquire": "^2.0.0",
    "sinon": "^10.0.0",
    "tmp": "^0.2.0",
    "ts-loader": "^9.0.0",
    "typescript": "^3.8.3",
    "uuid": "^8.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "yargs": "^16.0.0"
  }
